# ============================
# MySQL Database Configuration
# ============================

MYSQL_PORT=3306                      # Default MySQL port for client connections
MYSQLX_PORT=33060                    # MySQL X Protocol port (used for MySQL Shell and X DevAPI)
MYSQL_ROOT_PASSWORD=rootpassword     # Root user password (avoid hardcoding in production)
MYSQL_USER=myuser                    # Regular MySQL user
MYSQL_PASSWORD=mypass                # Password for the regular user (use secrets management in production)
MYSQL_DATABASE=mydatabase         
